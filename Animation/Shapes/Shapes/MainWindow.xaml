<Window x:Class="Shapes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Shapes"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
	<Canvas>
		<Rectangle Width="50"
				   Height="50"
				   Fill="Blue"
				   Canvas.Top="50"
				   Canvas.Left="50"
				   Stroke="Red"
				   StrokeThickness="5"/>

		<Ellipse Width="50"
				 Height="50"
				 Fill="Green"
				 Canvas.Top="50"
				 Canvas.Left="150"
				 Stroke="Red"
				 StrokeThickness="2"/>

		<Line X1="0"
			  Y1="0"
			  X2="25"
			  Y2="50"
			  Stroke="Red"
			  StrokeThickness="10"/>

		<Polygon
			Fill="Green"
			Points="100,100,200,150,125,175,175,250"
			Stroke="Blue"
			StrokeThickness="5" />
		<Image Height="100" Canvas.Left="505.068" Canvas.Top="272.099" Width="100"/>
		<Ellipse Fill="#FF389C8A" Height="100" Canvas.Left="484.854" Stroke="Black" Canvas.Top="116.009" Width="100"/>

	</Canvas>
</Window>
